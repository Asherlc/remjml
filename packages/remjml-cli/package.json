{
  "name": "remjml-cli",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "main": "./src/index.ts",
  "dependencies": {
    "remjml": "*",
    "unified-args": "10.0.0",
    "ts-node": "10.9.1"
  },
  "bin": {
    "remjml": "src/index.ts"
  },
  "scripts": {
    "clean": "tsc --build --clean",
    "build": "tsc --build",
    "watch": "tsc --build --watch",
    "process": "NODE_OPTIONS='--loader ts-node/esm' ts-node src/index.ts"
  }
}
